{% extends 'OroUIBundle:actions:index.html.twig' %}
{% import 'OroUIBundle::macros.html.twig' as UI %}
{% set gridName = 'parents-issue-grid' %}
{% set pageTitle = 'oro.issue.entity_plural.label'|trans %}

{% block navButtons %}
    {% if resource_granted('oro_issue_create') %}
        {{ UI.addButton({
        'path' : path('oro_issue_create'),
        'entity_label': 'oro.issue.entity.label'|trans
        }) }}
    {% endif %}

    {% include 'OroImportExportBundle:ImportExport:buttons.html.twig' with {
        entity_class:            entity_class,
        importProcessor:         'oro_issue',
        exportProcessor:         'oro_issue',
        exportTemplateProcessor: 'oro_issue',
        dataGridName:             gridName
    } %}

{% endblock %}
